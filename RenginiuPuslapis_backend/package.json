{
  "version": "0.0.0",
  "name": "renginiupuslapis-backend",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "watch": "nodemon ./bin/www",
    "test": "jest",
    "prettier-format": "prettier --write .",
    "prettier-check": "prettier --check .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": [
      "yarn eslint .",
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "express-async-handler": "^1.2.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.4",
    "morgan": "~1.9.1"
  },
  "packageManager": "yarn@3.2.0",
  "devDependencies": {
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1"
  },
  "eslintConfig": {
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true,
      "jest/globals": true
    },
    "plugins": [
      "jest"
    ],
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "rules": {}
  }
}
